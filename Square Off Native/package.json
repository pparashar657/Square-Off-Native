{
  "name": "@amzn/square-off-native",
  "version": "1.0.0",
  "description": "This is an Octane-generated package template for the SquareOffNATIVE React Native application, fully integrated with Brazil and ART Native.",
  "main": "src/index.js",
  "license": "UNLICENSED",
  "scripts": {
    "start": "ENABLE_METRO_BB_WS=1 node node_modules/react-native/local-cli/cli.js start",
    "prepublishOnly": "amzn-build-bundles",
    "pretest": "npm run lint",
    "lint": "node_modules/tslint/bin/tslint --project tsconfig.json",
    "test": "jest --colors --config jestconfig.json",
    "format": "prettier --write '**/*.{md,json}' '**/*.{js,jsx,ts,tsx}' '!.tmp/**'"
  },
  "rn-bundle-tools": {
    "appName": "SquareOffNATIVE",
    "bundleName": "main",
    "options": {
      "entryFile": "index.js"
    }
  },
  "devDependencies": {
    "@amzn/brazil": "*",
    "@amzn/brazil-metro-bundler-config": "*",
    "@amzn/react-native-bundle-tools": "*",
    "@types/jest": "^23.3.1",
    "@types/react": "16.3.x",
    "@types/react-native": "^0.55.x",
    "@types/react-test-renderer": "^16.0.1",
    "babel-jest": "^23.4.2",
    "babel-preset-react-native": "^4.0.0",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "prettier": "^1.14.2",
    "react-native-typescript-transformer": "^1.2.10",
    "react-navigation": "^2.9.3",
    "react-native-easy-toast": "1.1.x",
    "react-test-renderer": "^16.3.2",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "react": "16.3.1",
    "react-native": "0.55.3"
  }
}
